{
  "functions": {
    "pages/api/notifications.ts": {
      "maxDuration": 30
    },
    "pages/api/notifications/get.ts": {
      "maxDuration": 30
    },
    "pages/api/openapi.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/openapi.yaml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-yaml"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/openapi.yaml",
      "destination": "/api/openapi"
    }
  ]
}
